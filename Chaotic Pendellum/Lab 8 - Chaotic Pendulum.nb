   RowBox[{"(*", " ", 
    RowBox[{"first", " ", "point", " ", "of", " ", "the", " ", "algorithm"}], 
    " ", "*)"}], "\n", "\n", 
   RowBox[{
    RowBox[{"xminus", " ", "=", " ", "x0"}], ";"}], "\n", 
