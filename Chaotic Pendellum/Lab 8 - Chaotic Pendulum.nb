      RowBox[{"n\[Pi]", ".", " ", "a"}], " ", "centered", " ", "formula", " ",
       "is", " ", "used", "\n", "\t  ", "to", " ", "approximate", " ", "the", 
      " ", "second", " ", 
      RowBox[{"derivative", ".", " ", "\n", "\n", "   ", "parameters"}], " ", 
      "needed", " ", "to", " ", "test", " ", "when", " ", "to", " ", "store", 
      "\n", "\t\t ", "the", " ", 
      RowBox[{"data", "."}]}]}], " ", "*)"}], "\n", 
   StyleBox["\n",
    Background->RGBColor[1, 1, 0]], 
   StyleBox[
    RowBox[{
     RowBox[{"TimeTest0", " ", "=", 
      RowBox[{"2", "*", 
       RowBox[{"Pi", "/", "omega2"}]}]}], " ", ";"}],
    Background->RGBColor[1, 1, 0]], 
   StyleBox["\n",
    Background->RGBColor[1, 1, 0]], 
   StyleBox[
    RowBox[{
     RowBox[{"TimeTest", "=", "TimeTest0"}], ";"}],
    Background->RGBColor[1, 1, 0]], 
   StyleBox["\n",
